{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/public/index.php"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Content-Type", "value": "text/html; charset=utf-8" }
        ]
      }
    ],
    "trailingSlash": true
  }
