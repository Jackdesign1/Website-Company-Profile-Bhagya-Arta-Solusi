{
    "functions": {
      "public/index.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.php",
        "headers": {
          "Content-Type": "text/html; charset=utf-8"
        }
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/picture/(.*)",
        "dest": "/public/picture/$1"
      }
    ]
  }
